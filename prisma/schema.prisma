datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id Int @default(autoincrement()) @id
  email String @unique @db.VarChar(125)
  name String? @db.VarChar(125)
  password String @db.VarChar(255)
  refresh_token String
}
